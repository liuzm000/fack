<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd">
	<defaultCache maxElementsInMemory="2" eternal="false"
		timeToIdleSeconds="1" timeToLiveSeconds="1" overflowToDisk="false"
		memoryStoreEvictionPolicy="LRU" />
	<cache name="softVersion" timeToIdleSeconds="600" timeToLiveSeconds="1200" maxElementsInMemory="10000" eternal="false" 
		memoryStoreEvictionPolicy="LRU" overflowToDisk="false" />
</ehcache>

